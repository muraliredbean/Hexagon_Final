@import url('font.css');
html,
body,
.container,
.content,
section {
  height: 100%;
  margin: 0;
  padding: 0;
}
body{
 overflow-x:hidden;
}

.content > nav {
  position: fixed;
  z-index: 1;
  right: 100px;
  top: 50%;
  -webkit-transform: translateY(-50%);
  -moz-transform: translateY(-50%);
  -ms-transform: translateY(-50%);
  transform: translateY(-50%);
}

.content > nav a {
  display: block;
  position: relative;
  height: 50px;
}

.content > nav a:after {
  content: '';
  position: absolute;
  width: 10px;
  height: 10px;
  border-radius: 50%;
  background-color: #FFF;
}

.content > nav a:hover:after { background: #FFF; }

.content > nav a.current:after {background: #d5d110; border: 2px solid #FFF; }

.content section {
  position: relative;
  background-position: top center;
  background-repeat: no-repeat;
  background-size: cover;
  background-attachment: fixed;
}
.section-name{
	font-size:60px;
	font-weight: bold;
	text-align:center;
	font-family: 'TitilliumWeb-Regular';
	margin:0;
}
.section-name span{
	display:inline-block;
	position:relative;
	padding-bottom:5px;
	line-height:80px;
}
#content {
	width: 100%;
	margin-left: auto;
	margin-right: auto;
	cursor:pointer;
	text-align:center;
}
#outer-circle {
    position: absolute;
    background: transparent;
    border-radius: 50%;
    height: 100px;
    width: 100px;
    border: 1px solid #fff;
	bottom:0px;
	margin-bottom:50px;
}
#outer-circle1 {
    position: absolute;
    background: transparent;
    border-radius: 50%;
    height: 100px;
    width: 100px;
    border: 1px solid #fff;
	bottom:0px;
	margin-bottom:100px;
}
#inner-circle {
	position: relative;
    background: transparent;
    border-radius: 50%;
    height: 60px;
    width: 60px;
    margin: 0px 0px 0px 20px;
    top: 70%;
    margin-top: -50px;
    border: 2px solid #fff;
	text-align:center;

}
#inside-content{
    content: '';
    display: inline-block;
    margin-top: 1.2em;
    width: 1.2em;
    height: 1.2em;
    border-top: 0.2em solid #FFF;
    border-right: 0.2em solid #FFF;
    -moz-transform: rotate(135deg);
    -webkit-transform: rotate(135deg);
    transform: rotate(135deg);
    text-align: center;
	}
.section-name span:before,
.section-name span:after{
	content:"";
	width:100%;
	height:10px;
	position:absolute;
	left:0;
}
.section-name span:before{
	bottom:0;
}
.section-name span:after{
	bottom:2px;
}
