/* Banner Button Division and Button style */
.button_div{
	width:100%;
	text-align:center;
	padding-top:30px;
}
.section_button_div{
	width:100%;
	text-align:center;
	padding-top:50px;
	padding-bottom:140px;
}
.banner_button {
	background-color:transparent;
	-moz-border-radius:40px;
	-webkit-border-radius:40px;
	border-radius:40px;
	border:2px solid #ffffff;
	display:inline-block;
	cursor:pointer;
	color:#ffffff;
	font-family:'TitilliumWeb-SemiBold';
	font-size:20px;
	height:40px;
	padding-left:40px;
	padding-right:40px;
	text-align:center;
	text-decoration:none;
}

.banner_button:hover {
	background-color:transparent;
}
.banner_button:active {
	position:relative;
	top:1px;
}
.banner_button:focus {
outline:none;
}
.section_button {
	background-color:transparent;
	-moz-border-radius:40px;
	-webkit-border-radius:40px;
	border-radius:40px;
	border:2px solid #0072c7;
